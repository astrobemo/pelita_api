generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model nd_bank_list {
  id             Int      @id @default(autoincrement())
  nama_bank      String?  @db.VarChar(100)
  no_rek_bank    String?  @db.VarChar(100)
  tipe_trx_1     String?  @db.VarChar(7)
  tipe_trx_2     String?  @db.VarChar(7)
  status_default Boolean?
  status_aktif   Boolean? @default(true)
  user_id        Int      @db.SmallInt
  updated_at     DateTime @default(now()) @db.DateTime(0)
}

model barang {
  id             Int      @id @default(autoincrement())
  jenis_barang   String?  @default(dbgenerated("(POLYESTER)")) @db.VarChar(100)
  nama           String?  @db.VarChar(100)
  nama_jual      String?  @db.VarChar(100)
  harga_jual     Int?
  harga_beli     Decimal? @db.Decimal(10, 2)
  satuan_id      Int
  qty_warning    Int      @default(500)
  deskripsi_info String?  @db.VarChar(500)
  status_aktif   Int?     @default(1)
  tipe_qty       Boolean? @default(true)
  created_at     DateTime @default(now()) @db.DateTime(0)
  updated_at     DateTime @default(now()) @db.DateTime(0)
  satuan satuan @relation(fields: [satuan_id], references: [id])
  penjualan_detail penjualan_detail[]
  pembelian_detail pembelian_detail[]

  @@index([nama], map: "nama")
  @@index([nama_jual], map: "nama_jual")

  @@map("nd_barang")
}

model barang_beli {
  id           Int      @id @default(autoincrement())
  nama         String?  @db.VarChar(100)
  barang_id    Int?
  user_id      Int?     @db.SmallInt
  status_aktif Int      @default(1) @db.TinyInt
  created_at   DateTime @default(now()) @db.DateTime(0)
  updated_at   DateTime @default(now()) @db.DateTime(0)

  pembelian_detail pembelian_detail[]

}

model nd_barang_forecasting_data {
  id         Int       @id @default(autoincrement())
  barang_id  Int?      @db.SmallInt
  warna_id   Int?      @db.SmallInt
  period     DateTime? @db.Date
  qty        Int?      @db.MediumInt
  user_id    Int?      @db.SmallInt
  created_at DateTime? @default(now()) @db.DateTime(0)
  updated    DateTime? @default(now()) @db.DateTime(0)
}

model nd_barang_forecasting_keterangan {
  id         Int       @id @default(autoincrement())
  barang_id  Int?      @db.SmallInt
  warna_id   Int?      @db.SmallInt
  period     DateTime? @db.Date
  keterangan String?   @db.VarChar(200)
  user_id    Int?      @db.SmallInt
  created    DateTime? @default(now()) @db.DateTime(0)
  updated    DateTime? @default(now()) @db.DateTime(0)
}

model nd_barang_group {
  id              Int      @id @default(autoincrement())
  barang_id       Int?     @db.SmallInt
  barang_id_induk Int?     @db.SmallInt
  user_id         Int?     @db.SmallInt
  created_at      DateTime @default(now()) @db.DateTime(0)
  updated_at      DateTime @default(now()) @db.DateTime(0)
  status_aktif    Boolean  @default(true)
}

model nd_barang_harga_history {
  id         Int       @id @default(autoincrement())
  tanggal    DateTime? @db.Date
  barang_id  Int?
  harga_beli Decimal?  @db.Decimal(15, 2)
  harga_jual Decimal?  @db.Decimal(15, 2)
  created_at DateTime  @default(now()) @db.DateTime(0)
  updated_at DateTime  @default(now()) @db.DateTime(0)
  user_id    Int?      @db.SmallInt
}

model nd_barang_warna_temp {
  id        Int  @id @default(autoincrement())
  barang_id Int?
  warna_id  Int?
}

model nd_bayar_dp {
  id   Int     @id @default(autoincrement())
  nama String? @db.VarChar(200)
}

model nd_bday_user {
  id          Int       @id @default(autoincrement())
  user_id     Int?
  ulang_tahun DateTime? @db.Date
}

model nd_close_day {
  id            Int       @id @default(autoincrement())
  tanggal_start DateTime? @db.Date
  tanggal_end   DateTime? @db.Date
  user_id       Int?
}

model nd_controller {
  id   Int     @id @default(autoincrement())
  name String? @db.VarChar(100)
}

model customer {
  id                   Int       @id @default(autoincrement())
  customer_type_id     Boolean   @default(true)
  nama                 String?   @db.VarChar(70)
  alias                String    @db.VarChar(300)
  alamat               String?   @db.VarChar(1000)
  telepon1             String?   @db.VarChar(500)
  telepon2             String?   @db.VarChar(50)
  npwp                 String?   @db.VarChar(200)
  nik                  String?   @db.VarChar(16)
  kota                 String?   @db.VarChar(200)
  provinsi             String?   @db.VarChar(100)
  kode_pos             String?   @db.VarChar(20)
  email                String?   @db.VarChar(200)
  contact_person       String    @db.VarChar(500)
  tempo_kredit         Int?      @db.TinyInt
  warning_kredit       Int?      @db.TinyInt
  limit_warning_type   Int?      @db.TinyInt
  limit_amount         Int?
  limit_atas           Int?
  limit_warning_amount Int?
  status_aktif         Boolean   @default(true)
  img_link             String?   @db.VarChar(500)
  npwp_link            String?   @db.VarChar(500)
  ktp_link             String?   @db.VarChar(500)
  medsos_link          String?   @db.VarChar(2000)
  updated_at           DateTime  @default(now()) @db.DateTime(0)
  tipe_company         String?   @db.VarChar(3)
  blok                 String    @default(dbgenerated("(-)")) @db.VarChar(100)
  no                   String?   @default(dbgenerated("(-)")) @db.VarChar(100)
  rt                   String    @default(dbgenerated("(000)")) @db.VarChar(4)
  rw                   String    @default(dbgenerated("(000)")) @db.VarChar(4)
  kecamatan            String    @default(dbgenerated("(-)")) @db.VarChar(100)
  kelurahan            String    @default(dbgenerated("(-)")) @db.VarChar(100)
  locked_status        Boolean   @default(true)
  user_id              Int       @default(0) @db.TinyInt
  verified_time        DateTime? @db.DateTime(0)
  customer_id_central  String?   @db.VarChar(50)

  rekam_faktur_pajak_detail rekam_faktur_pajak_detail[]
  penjualan penjualan[]

  @@map("nd_customer")
}

